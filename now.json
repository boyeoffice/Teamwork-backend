{
  "version": 2,
  "builds": [
  	{
	   "src": "./index.js",
	   "use": "@now/node-server"
    }
 ],
  "routes": [
    {
      "src": "/v1/auth/signin",
      "dest": "/index.js",
      "methods": ["POST"] 
    },
    {
      "src": "/v1/auth/create-user",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/v1/articles",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/v1/articles",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/v1/articles/:articleId",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/v1/article/:articleIds",
      "dest": "/index.js",
      "methods": ["PATCH"]
    },
    {
      "src": "/v1/articles/:articleId",
      "dest": "/index.js",
      "methods": ["DELETE"]
    },
    {
      "src": "/v1/articles/:articleId/comment",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/v1/category",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/v1/category",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/v1/category/:categoryId",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/v1/category/:categoryId",
      "dest": "/index.js",
      "methods": ["PATCH"]
    },
    {
      "src": "/v1/category/:categoryId",
      "dest": "/index.js",
      "methods": ["DELETE"]
    },
    {
      "src": "/v1/gifs",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/v1/gifs",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/v1/gifs/:gifId",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/v1/gifs/:gifId",
      "dest": "/index.js",
      "methods": ["DELETE"]
    },
    {
      "src": "/v1/gifs/:gifId",
      "dest": "/index.js",
      "methods": ["DELETE"]
    },
    {
      "src": "/v1/gifs/:gifId/comment",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/v1/feed",
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ]
}