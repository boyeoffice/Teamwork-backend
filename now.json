{
  "version": 2,
  "builds": [
  	{
	   "src": "./index.js",
	   "use": "@now/node-server"
    }
 ],
  "routes": [
    {
      "src": "/v1/auth/signin",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/v1/auth/create-user",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/v1/articles",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/v1/articles",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/v1/articles",
      "dest": "/index.js",
      "methods": ["PUT"]
    },
    {
      "src": "/v1/articles",
      "dest": "/index.js",
      "methods": ["DELETE"]
    }
  ]
}